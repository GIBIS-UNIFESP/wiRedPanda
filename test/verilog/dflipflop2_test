S_0x563febe71af0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x563febe46cf0 .scope module, "dflipflop2_tb" "dflipflop2_tb" 3 3;
 .timescale -9 -12;
L_0x7f9b2b900018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x563febe9d240_0 .net "output_led1_0_1", 0 0, L_0x7f9b2b900018;  1 drivers
